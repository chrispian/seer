{
  "name": "fs.diff.apply",
  "version": "1.0.0",
  "scope": "fs.write",
  "input_schema": {
    "type": "object",
    "properties": {
      "patch": {
        "type": "string"
      },
      "dry_run": {
        "type": "boolean",
        "default": true
      }
    },
    "required": [
      "patch"
    ]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "applied": {
        "type": "boolean"
      },
      "hunks": {
        "type": "integer"
      },
      "rejects": {
        "type": "integer"
      }
    }
  }
}
