{
  "name": "export.generate",
  "scope": "export.read",
  "input_schema": {
    "type": "object",
    "properties": {
      "entity": {
        "type": "string"
      },
      "query_ref": {
        "type": [
          "string",
          "null"
        ]
      },
      "template": {
        "type": "string"
      },
      "format": {
        "type": "string",
        "enum": [
          "md",
          "txt",
          "json",
          "csv",
          "xlsx",
          "pdf"
        ]
      },
      "params": {
        "type": "object"
      }
    },
    "required": [
      "format"
    ]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "artifact_id": {
        "type": "string"
      },
      "path": {
        "type": "string"
      },
      "mime": {
        "type": "string"
      },
      "sha256": {
        "type": "string"
      }
    }
  }
}
