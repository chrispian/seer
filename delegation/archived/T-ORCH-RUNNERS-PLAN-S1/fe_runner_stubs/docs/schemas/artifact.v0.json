{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Artifact v0",
  "type": "object",
  "required": [
    "schema_version",
    "uri",
    "kind"
  ],
  "properties": {
    "schema_version": {
      "const": "v0"
    },
    "uri": {
      "type": "string",
      "pattern": "^fe://"
    },
    "kind": {
      "type": "string"
    },
    "meta": {
      "type": "object"
    }
  }
}
