{
  "mcpServers": {
    "laravel-tool-crate": {
      "command": "/opt/homebrew/bin/php",
      "args": [
        "./artisan",
        "tool-crate:mcp"
      ]
    },
    "orch": {
      "command": "/opt/homebrew/bin/php",
      "args": [
        "./artisan",
        "orchestration:mcp"
      ]
    },
    "laravel-boost": {
      "command": "/opt/homebrew/bin/php",
      "args": [
        "./artisan",
        "boost:mcp"
      ]
    }
  }
}
