APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=seer
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

OPENAI_API_KEY=
OPENAI_ORGANIZATION=

# AI / Embeddings
EMBEDDINGS_ENABLED=false
EMBEDDINGS_PROVIDER=openai
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
EMBEDDINGS_VERSION=1

# AI Model Selection & Parameters
AI_DEFAULT_PROVIDER=openai
AI_DEFAULT_TEXT_MODEL=gpt-4o-mini
AI_FALLBACK_PROVIDER=ollama
AI_FALLBACK_TEXT_MODEL=llama3:latest

# Operation-specific AI provider overrides (optional)
# If not set, uses the default provider/model above

# Fragment enrichment (content enhancement)
AI_ENRICHMENT_PROVIDER=
AI_ENRICHMENT_MODEL=
AI_ENRICHMENT_ENABLED=true

# Type classification and inference
AI_CLASSIFICATION_PROVIDER=
AI_CLASSIFICATION_MODEL=
AI_CLASSIFICATION_ENABLED=true

# Text embeddings for search
AI_EMBEDDING_PROVIDER=
AI_EMBEDDING_MODEL=
AI_EMBEDDING_ENABLED=true

# AI-powered tag suggestions (disabled by default - uses rules)
AI_TAGGING_PROVIDER=
AI_TAGGING_MODEL=
AI_TAGGING_ENABLED=false

# AI-powered title generation (disabled by default)
AI_TITLE_GENERATION_PROVIDER=
AI_TITLE_GENERATION_MODEL=
AI_TITLE_GENERATION_ENABLED=false

# Deterministic AI Parameters
# Classification (type inference, etc.) - Low temperature for consistency
AI_CLASSIFICATION_TEMPERATURE=0.1
AI_CLASSIFICATION_TOP_P=0.95
AI_CLASSIFICATION_MAX_TOKENS=500

# Enrichment (content enhancement) - Moderate temperature for creativity
AI_ENRICHMENT_TEMPERATURE=0.3
AI_ENRICHMENT_TOP_P=0.95
AI_ENRICHMENT_MAX_TOKENS=1000

# AI-powered tag suggestions - Low temperature for consistent suggestions
AI_TAGGING_TEMPERATURE=0.2
AI_TAGGING_TOP_P=0.95
AI_TAGGING_MAX_TOKENS=300

# AI-powered title generation - Low temperature for consistent titles
AI_TITLE_GENERATION_TEMPERATURE=0.1
AI_TITLE_GENERATION_TOP_P=0.95
AI_TITLE_GENERATION_MAX_TOKENS=100

# Embedding parameters
AI_EMBEDDING_DIMENSIONS=

# Orchestration - Artifacts Storage
FE_ARTIFACTS_DISK=local
FE_ARTIFACTS_ROOT=orchestration/artifacts

# Orchestration - Message Retention
FE_MSG_RETENTION_DAYS=365

# Orchestration - Secret Redaction
FE_REDACTION_ENABLED=true
# FE_REDACT_PATTERNS=CUSTOM_SECRET_PATTERN,ANOTHER_PATTERN
